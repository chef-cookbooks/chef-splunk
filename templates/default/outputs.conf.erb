[tcpout]
defaultGroup = default
disabled=false

<% @outputs.each_pair do |name, output| -%>
[tcpout:<%= name %>]
server=<%= output['servers'].map { |s| "#{s['ipaddress']}:#{s['port']}" }.join(',') %>
<% output['attributes'].each_pair do |name, value| -%>
<%= name %> = <%= value %>
<% end -%>

<% end -%>
